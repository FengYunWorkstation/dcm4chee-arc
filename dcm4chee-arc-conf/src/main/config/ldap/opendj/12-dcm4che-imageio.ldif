# dcm4che Image IO extensions of the DICOM Application Configuration Data Model Hierarchy LDAP Schema
dn: cn=schema
objectClass: top
objectClass: ldapSubentry
objectClass: subschema
attributeTypes: ( 1.2.40.0.13.1.15.4.3.1 NAME 'dcmIIOFormatName'
  DESC 'Image IO Reader or Writer Format Name'
  EQUALITY caseExactMatch
  SUBSTR caseExactSubstringsMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15
  SINGLE-VALUE )
attributeTypes: ( 1.2.40.0.13.1.15.4.3.2 NAME 'dcmJavaClassName'
  DESC 'Fully qualified Java class or interface name'
  EQUALITY caseExactMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15
  SINGLE-VALUE )
attributeTypes: ( 1.2.40.0.13.1.15.4.3.3 NAME 'dcmPatchJPEGLS'
  DESC 'Patch JPEG-LS: "JAI2ISO", "ISO2JAI", "ISO2JAI_IF_APP_OR_COM"'
  EQUALITY caseExactMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15
  SINGLE-VALUE )
attributeTypes: ( 1.2.40.0.13.1.15.4.3.4 NAME 'dcmImageWriteParam'
  DESC 'Image Write parameter'
  EQUALITY caseExactMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15 )
objectClasses: ( 1.2.40.0.13.1.15.4.4.1 NAME 'dcmImageReaderFactory'
  DESC 'Image Reader Factory related information'
  SUP top STRUCTURAL
  MUST ( cn ) )
objectClasses: ( 1.2.40.0.13.1.15.4.4.2 NAME 'dcmImageReader'
  DESC 'Image Reader related information'
  SUP top STRUCTURAL
  MUST ( 
    dicomTransferSyntax $ 
    dcmIIOFormatName )
  MAY (
    dcmJavaClassName $
    dcmPatchJPEGLS ) )
objectClasses: ( 1.2.40.0.13.1.15.4.4.3 NAME 'dcmImageWriterFactory'
  DESC 'Image Reader Factory related information'
  SUP top STRUCTURAL
  MUST ( cn ) )
objectClasses: ( 1.2.40.0.13.1.15.4.4.5 NAME 'dcmImageWriter'
  DESC 'Image Reader related information'
  SUP top STRUCTURAL
  MUST ( 
    dicomTransferSyntax $ 
    dcmIIOFormatName )
  MAY (
    dcmJavaClassName $
    dcmPatchJPEGLS $
    dcmImageWriteParam ) )
